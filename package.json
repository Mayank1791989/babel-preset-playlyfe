{
  "name": "babel-preset-playlyfe",
  "version": "0.0.0-auto-generate-by-semantic-release",
  "description": "Babel preset for playlyfe",
  "main": "lib/index.js",
  "author": "mayank@playlyfe.com",
  "contributors": [
    "Kumar Harsh <khs@playlyfe.com> (https://github.com/kumarharsh)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayank1791989/babel-preset-playlyfe.git"
  },
  "license": "MIT",
  "keywords": [
    "babel-preset",
    "react",
    "es2015",
    "playlyfe"
  ],
  "scripts": {
    "test": "npm run check-code && npm run testonly -- --coverage",
    "testonly": "cross-env NODE_ENV=test jest",
    "check-code": "npm run lint && npm run flow",
    "flow": "flow check",
    "flow-typed-update": "flow-typed update",
    "lint": "eslint src",
    "report-coverage": "codecov",
    "build": "rm -rf lib && babel ./src --out-dir ./lib --ignore '**/__tests__/**,**/node_modules/**'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "homepage": "https://github.com/Mayank1791989/babel-preset-playlyfe#readme",
  "files": [
    "lib",
    "README.md"
  ],
  "dependencies": {
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-syntax-flow": "6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators": "6.13.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.20.0",
    "babel-plugin-transform-proto-to-assign": "6.9.0",
    "babel-plugin-transform-react-display-name": "6.8.0",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "modify-babel-preset": "3.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-flow": "1.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "codecov": "1.0.1",
    "commitizen": "2.8.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.11.1",
    "eslint-config-airbnb-base": "10.0.1",
    "flow-bin": "0.36.0",
    "flow-typed": "2.0.0",
    "ghooks": "1.3.2",
    "jest-cli": "17.0.3",
    "react": "15.4.1",
    "react-intl": "2.1.5",
    "semantic-release": "6.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "testRegex": "__tests__/.*-test.js$",
    "testEnvironment": "node",
    "testPathDirs": [
      "src/"
    ]
  }
}
